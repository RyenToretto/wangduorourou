{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "first-ng-lib": {
      "projectType": "library",
      "root": "projects/first-ng-lib",
      "sourceRoot": "projects/first-ng-lib/src",
      "prefix": "kjf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/first-ng-lib/tsconfig.lib.json",
            "project": "projects/first-ng-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/first-ng-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/first-ng-lib/src/test.ts",
            "tsConfig": "projects/first-ng-lib/tsconfig.spec.json",
            "karmaConfig": "projects/first-ng-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/first-ng-lib/tsconfig.lib.json",
              "projects/first-ng-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "1c5deb00-892d-4c43-8826-6c26abf29e50"
  },
  "defaultProject": "first-ng-lib"
}